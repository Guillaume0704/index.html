    ISBLANK({!$Record.Assechmur_Prixradiateurademonter__c}),
    {!$Record.Assechmur_PrixtotalVMC__c} * (1 + {!$Record.Assechmur_Marge__c}/100),
    ({!$Record.Assechmur_PrixtotalVMC__c} + {!$Record.Assechmur_Prixradiateurademonter__c}) * (1 + {!$Record.Assechmur_Marge__c}/100)
